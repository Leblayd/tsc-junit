{
  "name": "tsc-junit",
  "version": "1.0.0",
  "main": "dist/index.js",
  "description": "JUnit report generator for typescript",
  "repository": "https://github.com/iam-medvedev/tsc-junit.git",
  "homepage": "https://github.com/iam-medvedev/tsc-junit#readme",
  "bugs": "https://github.com/iam-medvedev/tsc-junit/issues",
  "author": "Ilya Medvedev <ilya@medvedev.im>",
  "license": "WTFPL",
  "scripts": {
    "dev": "ts-node ./src",
    "commit": "git-cz"
  },
  "dependencies": {
    "@aivenio/tsc-output-parser": "^2.1.1"
  },
  "devDependencies": {
    "@types/prettier": "^2",
    "cz-conventional-changelog": "^3.3.0",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.3",
    "prettier": "^2.5.1",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "keywords": [
    "tsc",
    "typescript",
    "junit",
    "report"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.1.1"
}
